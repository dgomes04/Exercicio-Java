package lista2;

import java.util.Scanner;

public class atividade5 {

	public static void main(String[] args) {
		Scanner ent = new Scanner(System.in);
		int l, ver;
		
		System.out.println("Digite o número da série de Fibonacci:");
		l = ent.nextInt();
		
		for(ver = 0; ver<= l; ver++) {
			while(ver <= 39) {
				System.out.println(l);
			}
		
		}

	}

}
