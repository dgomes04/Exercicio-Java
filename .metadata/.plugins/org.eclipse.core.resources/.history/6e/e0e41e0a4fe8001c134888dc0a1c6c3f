package lista6;

import java.util.Scanner;

public class atividade6 {
	
	public static void proced(int n) {
		if(n==1) {
			System.out.println("*");
		}else {
			System.out.println();
			for(int i=0;i<n;i++) {
				System.out.print("*");
				
			}
			
			proced(n-1);
		}
	}

	public static void main(String[] args) {
		Scanner ent = new Scanner(System.in);
		int  n =5;

		proced(n);
	}

}
